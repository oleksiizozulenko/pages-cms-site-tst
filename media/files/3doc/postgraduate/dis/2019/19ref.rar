<script>
const menuToggle = document.getElementById('menu-toggle');
const navLinks = document.getElementById('nav-links');
const submenuParents = document.querySelectorAll('.has-submenu');

menuToggle.addEventListener('click', () => {
  navLinks.classList.toggle('active');
});

//відкриття лише одного підменю одночасно
submenuParents.forEach((parent) => {
  const link = parent.querySelector('.submenu-link');

  link.addEventListener('click', (e) => {
    if (window.innerWidth <= 660) {
      if (!parent.classList.contains('active')) {
        e.preventDefault(); // Забороняє перехід при відкритті
        // Закриваємо всі інші підменю
        submenuParents.forEach((el) => {
          el.classList.remove('active');
        });
        // Відкриваємо поточне
        parent.classList.add('active');
      } else {
        // Друге натискання — дозволяє перейти за посиланням
      }
    }
  });
});

const modal_leng = document.getElementById("modal_leng");
const closeBtn_lang = modal_leng.querySelector(".close_leng");

// Закрити при натисканні на ✖
closeBtn_lang.addEventListener("click", () => {
  modal_leng.style.display = "none";
});

// Закрити при натисканні поза вікном
window.addEventListener("click", (e) => {
  if (e.target === modal_leng) {
    modal_leng.style.display = "none";
  }
});

function langvige_cg(){
	modal_leng.style.display = "flex";
}
</script>

